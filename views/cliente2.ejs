                <img class="cards" src="/uploads/<%= shirt.imagen %>">
                <h6 class="card-title"><%= shirt.name %></h6>

                <a class="btn" href="/buy/<%= shirt.producto_id %>">Comprar</a>
            </div>
            <% }) %>

        </div>
    </div>
    <div>


        <div class="container-fluid footer">
            <div class="row">
                <div class="col-4">
                    <h5 class="colorL">Preguntas</h5>
